{
  "manifest_version": 2,

  "name": "Redmine Bug Tracker",
  "description": "Helps Tracking Bug within Redmine",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com https://redmine.bovpg.net; object-src 'self'",
	
  "permissions": [
      "activeTab",
	  "storage",
      "https://redmine.bovpg.net/"
  ]
}
